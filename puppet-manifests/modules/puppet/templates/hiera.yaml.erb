---
:backends:
<% @hiera_backends.each do |backend| %>
  - <%= backend %>
<% end %>
:yaml:
  :datadir: <%= @hiera_yaml_datadir %>
:json:
  :datadir: <%= @hiera_json_datadir %>
:hierarchy:
<% @hiera_hierarchy.each do |element| %>
  - <%= element %>
<% end %>
:logger: <%= @hiera_logger %>
:merge_behavior: <%= @hiera_merge_behavior %>
